<project name="EJB3 container">
	<target name="development checkout" description="Checkouts required plugins for development when using Eclipse default plugins">
		<antcall target="clear" />
		<antcall target="development" />
	</target>
	<target name="clear">
		<delete dir="plugins" />
		<mkdir dir="plugins" />
	</target>
	<target name="development">
		<osgi.release osgirepository="com.netappsid.tools.releaser.s3.S3OSGIRepository" dir="plugins">
			<plugin symbolicname="com.netappsid.datasource" version="2.1.1" />
			<plugin symbolicname="com.netappsid.org.apache.naming" version="6.0.20" />
			<plugin symbolicname="org.objectweb.asm" version="3.1.0" />
			<plugin symbolicname="com.netappsid.xml" version="1.0.0" />
			<plugin symbolicname="com.atomikos" version="3.5.9" />
			<plugin symbolicname="com.atomikos.transactions.properties" version="1.0.1" />
			<plugin symbolicname="com.google.common" version="1.0.0" />
			<plugin symbolicname="com.thoughtworks.xstream" version="1.0.0" />
			<plugin symbolicname="com.netappsid.javassist" version="3.9.0" />
			<plugin symbolicname="javax.persistence" version="1.99.0" />
			<plugin symbolicname="javax.transaction" version="1.1.1" />
			<plugin symbolicname="com.netappsid.javaee" version="5.0.0" />
			<plugin symbolicname="com.netappsid.org.hibernate" version="3.3.2" />
			<plugin symbolicname="slf4j.api" version="1.5.8" />
			<plugin symbolicname="slf4j.log4j12" version="1.5.8" />
			<plugin symbolicname="com.netappsid.common" version="2.1.0" />

			<plugin symbolicname="com.jacob" version="1.15.0" />
			<plugin symbolicname="com.jidesoft" version="2.4.5" />
			<plugin symbolicname="bsh" version="1.0.0" />
			<plugin symbolicname="com.netappsid.com.jgoodies.binding" version="2.0.6" />
			<plugin symbolicname="com.netappsid.com.jgoodies.forms" version="1.2.1" />
			<plugin symbolicname="com.netappsid.com.jgoodies.validation" version="2.0.1" />
			<plugin symbolicname="org.jscience" version="3.2.0" />
			<plugin symbolicname="com.springsource.org.objenesis" version="1.0.0" />
			<plugin symbolicname="com.springsource.javax.mail" version="1.4.1" />
