			<plugin symbolicname="javax.xml" version="1.3.4" />
			<plugin symbolicname="com.netappsid.net.sf.cglib" version="2.2.0" />
			<plugin symbolicname="javax.xml.rpc" version="1.1.0" />
			<plugin symbolicname="com.springsource.org.apache.commons.logging" version="1.1.1" />
